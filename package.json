{
  "name": "xynga-web-services",
  "version": "2.0.2",
  "description": "Holds all of the components classified under the label \"Web Services\"",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cd src && npm run compile",
    "link-build": "npm link && cd testing && npm link xynga-web-services",
    "preversion": "npm run build",
    "postversion": "git push",
    "rebuild": "npm run build && npm start",
    "setup": "cd src && npm i && cd .. && npm run build && cd testing && npm i && cd .. && npm run link-build",
    "start": "cd testing && start chrome http://localhost:4200 && ng serve",
    "test": "cd testing && ng test --watch=false --code-coverage"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xynga/web-services.git"
  },
  "peerDependencies": {
    "@angular/animations": ">=4.0.0",
    "@angular/core": ">=4.0.0",
    "@angular/common": ">=4.0.0",
    "@angular/http": ">=4.2.4",
    "rxjs": ">=5.4.2",
    "@angular/platform-browser": ">=4.2.4",
    "@angular/platform-browser-dynamic": ">=4.2.4",
    "@angular/router": ">=4.2.4",
    "@ng-idle/core": "^6.0.0-beta.2",
    "angular2-notifications": "^1.0.2"
  }
}
